<script>
    indexCur()
    function indexCur() {
        switch (getweb()) {
            case 2: Subst(); break;
            case 3: Subst(); break;
            case 4: Subst(); break;
        }
        switch (index()) {
            case 1: $(".dh_bg ul li").eq(0).addClass("cur"); break;
            case 2: $(".dh_bg ul li").eq(1).addClass("cur"); break;
            case 3: $(".dh_bg ul li").eq(2).addClass("cur"); break;
            case 4: $(".dh_bg ul li").eq(3).addClass("cur"); break;
            case 5: $(".dh_bg ul li").eq(4).addClass("cur"); break;
            case 6: $(".dh_bg ul li").eq(5).addClass("cur"); break;
            case 7: $(".dh_bg ul li").eq(6).addClass("cur"); break;
        }
    }
    function index() {
        var pathname = document.location.pathname
        if (pathname.indexOf('about') >= 0) return 2
        else if (pathname.indexOf('news') >= 0) return 3
        else if (pathname.indexOf('html') >= 0) return 3
        else if (pathname.indexOf('team') >= 0) return 4
        else if (pathname.indexOf('case') >= 0) return 5
        else if (pathname.indexOf('part') >= 0) return 6
        else if (pathname.indexOf('notify') >= 0) return 7
        else return 1
    }
    console.log('hello?')
    function IeSubst() {
        var Path = document.location
        var pathname = document.location.pathname
        if (pathname.indexOf('HomeS') >= 0) {
            var url = 'http://www.qhimi.com/HomeS/' + pathname.substring(6);
            location.href = url
        }
    }
    function Subst() {
        var Path = document.location
        var pathname = document.location.pathname
        if (pathname.indexOf('HomeS') >= 0) {
            var url = 'http://www.qhimi.com/Home' + pathname.substring(6);
            location.href = url
        }
    }
    function getweb() {
        if (!!window.ActiveXObject || "ActiveXObject" in window) {
            return 1;
        }
        if(navigator.userAgent.indexOf("Firefox") != -1) {
            return 2;
        }
        if (navigator.userAgent.indexOf("Chrome") != -1) {
            return 3;
        }
        if (navigator.userAgent.indexOf("Safari") != -1) {
            return 4;
        }
    }
</script>


